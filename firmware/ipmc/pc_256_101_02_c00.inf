[Board Point-to-Point Connectivity]
Base,1, 0 = Base, 0

# The *BASE-BX* entries in the fabric P2P records below 
# are left for backward compatibility with older boards.
# (The -BX specs were defined by PICMG prior to the equivalent
#  -KX specs by IEEE.)
#
# The order of these is defined in PICMG 3.1 R2.0 where, again
# for backward compatibility with old Shelf Managers, the 
# Ethernet-10Gbs entries follow the Ethernet entries.
# If the Shelf Manager understands PICMG 3.1 R2.0 it will
# re-order these to put the faster protocols first.

[Board Point-to-Point Connectivity]
Vendor Format Version = 1

# 1000BASE-KX on Port 0
Fabric,1, 0 = Ethernet,3
Fabric,2, 0 = Ethernet,3
Fabric,3, 0 = Ethernet,3
Fabric,4, 0 = Ethernet,3

# 1000BASE-BX on Port 0
Fabric,1, 0 = Ethernet,0
Fabric,2, 0 = Ethernet,0
Fabric,3, 0 = Ethernet,0
Fabric,4, 0 = Ethernet,0

[Board Point-to-Point Connectivity]
